<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project.Controllers.Settings">
   <children>
      <Label layoutX="20.0" layoutY="29.0" text="Username or Email">
         <font>
            <Font name="Century" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="265.0" layoutY="33.0" prefHeight="25.0" prefWidth="270.0" />
      <Label layoutX="20.0" layoutY="73.0" prefHeight="32.0" prefWidth="227.0" style="-fx-alignment: center;" text="Password">
         <font>
            <Font name="Century" size="25.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="265.0" layoutY="77.0" prefHeight="25.0" prefWidth="270.0">
      <tooltip>
         <Tooltip fx:id="tip" />
      </tooltip>
      </PasswordField>
      <HBox layoutX="36.0" layoutY="137.0" prefHeight="44.0" prefWidth="528.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#fbSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="Facebook" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#instaSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="Instagram" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#twiSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="Twitter" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox layoutX="36.0" layoutY="212.0" prefHeight="44.0" prefWidth="528.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#gitSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="Github" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#liSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="Linked In" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#iitSet" prefHeight="44.0" prefWidth="170.0" styleClass="buttonRed" stylesheets="@style.css" text="IIT LMS" textFill="WHITE">
               <font>
                  <Font name="Candara" size="20.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Button layoutX="546.0" layoutY="77.0" mnemonicParsing="false" onAction="#show" prefHeight="25.0" prefWidth="45.0" text="Show">
      </Button>
   </children>
</AnchorPane>
